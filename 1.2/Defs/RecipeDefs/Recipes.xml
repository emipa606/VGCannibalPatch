<?xml version="1.0" encoding="UTF-8"?>
<Defs>
  <!-- Stirfry -->
  <RecipeDef ParentName="CookMealBase" Name="CAN_Stirfrymeal">
    <defName>CAN_Stirfrymeal</defName>
    <label>Cook four cannibal stir-fried meals</label>
    <description>Cooks a stir-fried meal from a combination of human meat and raw plant ingredients. \n\nMakes enough for 4 meals.  \n\nFood Effects: Manipulation and Blood Filtration.</description>
    <jobString>Cooking cannibal stir-fried meals.</jobString>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <workAmount>1500</workAmount>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
    <allowMixingIngredients>true</allowMixingIngredients>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Meat_Human</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
      <li>
        <filter>
          <categories>
            <li>PlantFoodRaw</li>
            <li>CookingSupplies</li>
          </categories>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <products>
      <CAN_stirfry>4</CAN_stirfry>
    </products>
    <skillRequirements>
      <Cooking>10</Cooking>
    </skillRequirements>
    <recipeUsers>
      <li>ElectricStove</li>
      <li>FueledStove</li>
    </recipeUsers>
  </RecipeDef>
  <!-- Stew -->
  <RecipeDef ParentName="CookMealBase" Name="CAN_Cookstewpot">
    <defName>CAN_Cookstewpot</defName>
    <label>Cook eight pots of cannibal stew</label>
    <description>Slow cooks a pot of stew from a combination of human meat and raw plant ingredients. \n\nMakes enough for 8 meals. \n\nFood Effects: Consciousness, Metabolism, Blood Filtration, and Blood Pumping.</description>
    <jobString>Slow cooking cannibal stew.</jobString>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <workAmount>2000</workAmount>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
    <allowMixingIngredients>true</allowMixingIngredients>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Meat_Human</li>
          </thingDefs>
        </filter>
        <count>4</count>
      </li>
      <li>
        <filter>
          <categories>
            <li>PlantFoodRaw</li>
            <li>CookingSupplies</li>
          </categories>
        </filter>
        <count>4</count>
      </li>
    </ingredients>
    <products>
      <CAN_bowlstew>8</CAN_bowlstew>
    </products>
    <skillRequirements>
      <Cooking>15</Cooking>
    </skillRequirements>
    <recipeUsers>
      <li>ElectricStove</li>
      <li>FueledStove</li>
    </recipeUsers>
  </RecipeDef>
  <!-- Tacos -->
  <RecipeDef ParentName="CookMealBase">
    <defName>CAN_CookTacos</defName>
    <label>Cook five cannibal tacos</label>
    <description>Human meat and Cheese in a corn shell. Requires human meat, cheese, and cornmeal to make. \n\n Makes 5 servings. \n\nFood Effects: Manipulation and Metabolism.</description>
    <jobString>Preparing cannibal tacos.</jobString>
    <allowMixingIngredients>true</allowMixingIngredients>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Cornmeal</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>cheese</li>
          </thingDefs>
        </filter>
        <count>4</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Meat_Human</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
    </ingredients>
    <products>
      <CAN_Tacos>5</CAN_Tacos>
    </products>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Cornmeal</li>
        <li>cheese</li>
        <li>Meat_Human</li>
      </thingDefs>
      <specialFiltersToDisallow>
        <li>AllowRotten</li>
      </specialFiltersToDisallow>
    </fixedIngredientFilter>
    <workAmount>1000</workAmount>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <workSkill>Cooking</workSkill>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <recipeUsers>
      <li>ElectricStove</li>
      <li>FueledStove</li>
    </recipeUsers>
    <skillRequirements>
      <Cooking>7</Cooking>
    </skillRequirements>
  </RecipeDef>
  <!-- Salted Meat -->
  <RecipeDef ParentName="CookMealBase">
    <defName>CAN_SaltMeat</defName>
    <label>Salt ten human meats</label>
    <description>Preserve human meats in salt to prevent spoiling.</description>
    <jobString>Salting human meat.</jobString>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <workAmount>800</workAmount>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Meat_Human</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>ChunkSalt</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Meat_Human</li>
        <li>ChunkSalt</li>
      </thingDefs>
      <specialFiltersToDisallow>
        <li>AllowPlantFood</li>
        <li>AllowNonSmeltableWeapons</li>
        <li>AllowRotten</li>
      </specialFiltersToDisallow>
      <disallowedThingDefs>
        <li>CAN_SaltedMeat</li>
      </disallowedThingDefs>
    </fixedIngredientFilter>
    <products>
      <CAN_SaltedMeat>10</CAN_SaltedMeat>
    </products>
    <defaultIngredientFilter>
      <thingDefs>
        <li>Meat_Human</li>
        <li>ChunkSalt</li>
      </thingDefs>
    </defaultIngredientFilter>
    <workSkill>Cooking</workSkill>
    <recipeUsers>
      <li>Canningstove</li>
      <li>HandCanningstove</li>
    </recipeUsers>
  </RecipeDef>
